#include <iostream>
#include <cmath>

using namespace std;

double discriminant(double a, double b, double c)
{
	return sqrt(b) - 4*a*c;
}

int main() {
	double a, b, c;

	if(a == 0){
		if(b != 0) {
			std::cout << -1*c/b;
		}
	}
	else {
		double d;
		d = discriminant(a, b, c);
	}

	return 0;
}
