#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
using namespace std;

int main() {

	int n;
	cin >> n;

	vector<int> numbers;

	for(int i = 0; i < n; ++i){
		int number;
		cin >> number;

		numbers.push_back(abs(number));
	}

	sort(numbers.begin(), numbers.end());

	for(const auto& item : numbers){
		cout << item << ' ';
	}

	return 0;
}
