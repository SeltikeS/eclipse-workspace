#include <iostream>
#include <string>
#include <vector>
#include <map>

using namespace std;

map<char, int> BuildCharCounters(const string& s){
	map<char, int> char_counter;
	for(const auto& item : s){
		++char_counter[item];
	}
}

int main() {

	int n;
	cin >> n;

	cout << n << endl;

	vector<string> pair;



	for(int i = 0; i < n; ++i){
		cin >> pair[0] >> pair[1];

		cout << pair[0] << ' ' << pair[1] << endl;

//		if(BuildCharCounters(pairs[i][0]) == BuildCharCounters(pairs[i][1])) {
//			cout << "YES" << endl;
//		} else {
//			cout << "NO" << endl;
//		}
	}

	return 0;
}
