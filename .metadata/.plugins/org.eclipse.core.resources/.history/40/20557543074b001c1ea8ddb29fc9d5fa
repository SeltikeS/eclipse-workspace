#include <iostream>
#include <string>
#include <vector>
#include <map>

using namespace std;

map<char, int> BuildCharCounters(const string& s){
	map<char, int> char_counter;
	for(const auto& item : s){
		++char_counter[item];
	}
}

int main() {

	int n;
	cin >> n;

	vector<vector<string>> pairs;


	for(int i = 0; i < n; ++i){
		cin >> pairs[i][0] >> pairs[i][1];

		if(BuildCharCounters(pairs[i][0]) == BuildCharCounters(pairs[i][1])) {
			cout << "YES" << endl;
		} else {
			cout << "NO" << endl;
		}
	}

	return 0;
}
