#include <iostream>
#include <string>
#include <vector>
#include <map>

using namespace std;

//map< string, vector<char> >

int main() {

	int n;
	cin >> n;

	vector<vector<string>> pairs;

	for(int i = 0; i < n; ++i){
		cin >> pairs[i][0] >> pairs[i][1];
	}

	return 0;
}
