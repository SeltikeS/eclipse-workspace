#include <iostream>
#include <cmath>

using namespace std;

double discriminant(double a, double b, double c)
{
	return b*b - 4*a*c;
}



int main() {
	double a, b, c;

	if(a == 0){
		if(b != 0) {
			std::cout << -1*c/b;
		}
	}
	else {
		double d;
		d = discriminant(a, b, c);


		if(d > 0) {
			x1 = (-1*b + sqrt(d)) / (2*a);
			x2 = (-1*b - sqrt(d)) / (2*a);
			std::cout << x1 << ' ' << x2;
		}
		else if(d == 0) {
			x = -1*b / (2*a);
			std::cout << x;
		}
	}

	return 0;
}
